{
    "name": "jonesrussell/x-suite-laravel",
    "description": "X (Twitter) integration suite for Laravel â€” posting, analytics, trends, auto-replies, and content discovery with Vue 3 + Inertia admin UI.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "twitter",
        "x",
        "social-media",
        "inertia",
        "vue"
    ],
    "authors": [
        {
            "name": "Russell Jones",
            "email": "jonesrussell@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/console": "^12.0",
        "atymic/twitter": "^3.0",
        "guzzlehttp/guzzle": "^7.10",
        "phpoffice/phpspreadsheet": "^5.4"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "JonesRussell\\XSuite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JonesRussell\\XSuite\\Tests\\": "tests/",
            "JonesRussell\\XSuite\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JonesRussell\\XSuite\\XSuiteServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "pint --parallel",
        "lint:check": "pint --test",
        "test": "pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
